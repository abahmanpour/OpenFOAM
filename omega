FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $turbulentOmega;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    lowerWall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    frontWall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    backWall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    obstacleLeft
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    obstacleRight
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    obstacleFront
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    obstacleBack
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    obstacleTop
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    atmosphere
    {
        type            zeroGradient;

    }
}

// ************************************************************************* //
